{
  "name": "arzion",
  "version": "1.0.0",
  "description": "Proyecto de frontend para Arzion",
  "main": "/build/app.js",
  "scripts": {
    "build": "webpack --colors",
    "serve": "webpack-dev-server --content-base build/ --color --progress",
    "start": "npm run build && npm run serve",
